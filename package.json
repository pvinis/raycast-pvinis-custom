{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "pvinis-custom",
  "title": "pvinis-custom",
  "description": "Custom commands for pvinis",
  "icon": "extension-icon.png",
  "author": "pvinis",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "greeklish-to-greek",
      "title": "Greeklish->Greek",
      "description": "Convert Greeklish to Greek",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@nem035/gpt-3-encoder": "^1.1.7",
    "@raycast/api": "^1.89.1",
    "@raycast/utils": "^1.18.1",
    "openai": "^4.80.1"
  },
  "preferences": [
    {
      "name": "apikey",
      "title": "OpenAI API Key",
      "description": "Your OpenAI API key",
      "type": "password",
      "required": true,
      "placeholder": "sk-blablabla"
    }
  ],
  "devDependencies": {
    "@types/node": "22.12.0",
    "@types/react": "19.0.8",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "dev": "ray develop",
    "build": "ray build"
  }
}
